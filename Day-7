//Arrays

#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;
    cin>>n;
    int array[n];
    for (int i=0;i<n;i++){
        cin>>array[i];
    }
    
    for(int j=n-1;j>=0;j--)
    {
        cout<<array[j]<<"\t";
    }
    
       
    return 0;
}
